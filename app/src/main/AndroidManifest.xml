<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.android.example.quizapp">

    <application
        android:name="com.android.example.quizapp.models.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.android.example.quizapp.QuizSelection">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.android.example.quizapp.QuestionActivity">
        </activity>
        <activity android:name="com.android.example.quizapp.QizInfoActivity">
        </activity>
        <activity android:name="com.android.example.quizapp.QuizSummaryActivity">
        </activity>
    </application>

</manifest>